{
  "name": "aina-hugo-theme",
  "version": "1.0.0",
  "description": "Minimal Hugo theme for your blog",
  "main": "index.js",
  "scripts": {
    "dev": "hugo serve",
    "dev:css": "npx tailwindcss -i ./themes/aina/assets/css/tailwind.css -o ./themes/aina/static/css/styles.css -w",
    "build:css": "npx tailwindcss -i ./themes/aina/assets/css/tailwind.css -o ./themes/aina/static/css/styles.css -m"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:matusstafura/aina-hugo-theme.git"
  },
  "keywords": [
    "theme",
    "hugo",
    "go",
    "golang",
    "blog",
    "minimal"
  ],
  "author": "Matus Stafura",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "tailwindcss": "^3.0.23"
  }
}
